pipeline {
    agent any

   
        stage echo ('Install Dependencies') {
            steps {
                sh 'npm install'
            }
        }
        stage echo ('Run Tests') {
            steps {
                sh 'npm test || echo "No tests defined"'
            }
        }
        stage echo ('Build Application') {
            steps {
                sh 'npm run build'
            }
        }
   stages {
        stage echo ('Clone') {
            steps {
                git 'https://github.com/Annu881/Test.git' branch 'master'
            }
        }
    }
}
