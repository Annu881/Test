pipeline {
    agent any

   
        stage('Install Dependencies') {
            steps {
                sh 'npm install'
            }
        }
        stage('Run Tests') {
            steps {
                sh 'npm test || echo "No tests defined"'
            }
        }
        stage('Build Application') {
            steps {
                sh 'npm run build'
            }
        }
   stages {
        stage('Clone') {
            steps {
                git 'https://github.com/Annu881/Test.git' branch: 'master'
            }
        }
    }
}
